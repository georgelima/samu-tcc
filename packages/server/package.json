    "dotenv": "^7.0.0",
    "eres": "^1.0.1",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.1.0",
    "koa-router": "^7.4.0",
    "mongoose": "^5.7.5",
